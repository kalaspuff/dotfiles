{
    "workbench.startupEditor": "newUntitledFile",
    "editor.fontFamily": "Source Code Pro",
    "editor.fontLigatures": false,
    "files.autoSave": "onFocusChange",
    "files.trimTrailingWhitespace": true,
    "editor.tokenColorCustomizations": {
        "comments": "#72df9cea"
    },
    "workbench.colorTheme": "GitHub Dark",
    "editor.wordWrap": "off",
    "files.associations": {
        "*.phtml": "html"
    },
    "editor.minimap.enabled": true,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": true,
        "suppressNoRepositoryWarning": false,
    },
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": true
    },
    "python.pythonPath": "/usr/local/bin/python3",
    "python.autoComplete.addBrackets": true,
    "editor.renderWhitespace": "boundary",
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.editor.labelFormat": "short",
    "gitlens.codeLens.scopes": [
        "document"
    ],
    "isort.path": [
        "/Users/kalaspuff/.local/bin/isort"
    ],
    "python.linting.enabled": true,
    "python.linting.mypyEnabled": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.linting.mypyPath": "/Users/kalaspuff/.local/bin/mypy",
    "python.linting.flake8Path": "/Users/kalaspuff/.local/bin/flake8",
    "python.languageServer": "Pylance",
    "python.analysis.completeFunctionParens": true,
    "editor.cursorWidth": 2,
    "editor.cursorBlinking": "solid",
    "python.venvPath": "/Users/kalaspuff/Library/Caches/pypoetry/virtualenvs",
    "python.formatting.blackPath": "/Users/kalaspuff/.local/bin/black",
    "python.formatting.provider": "black",
    "docker.commands.build": [],
    "window.newWindowDimensions": "offset",
    "python.analysis.typeCheckingMode": "basic",
    "[python]": {
        "gitlens.codeLens.symbolScopes": [
            "!Module"
        ],
        "editor.wordBasedSuggestions": false,
        "editor.formatOnType": true
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "python.defaultInterpreterPath": "/usr/local/bin/python3",
    "snyk.yesWelcomeNotification": false,
    "snyk.token": "",
    "snyk.codeEnabled": false,
    "snyk.features.codeQuality": false,
    "snyk.features.codeSecurity": false,
    "snyk.yesCrashReport": false,
    "snyk.yesTelemetry": false,
    "snyk.features.openSourceSecurity": false,
    "snyk.advanced.automaticDependencyManagement": false,
    "editor.inlineSuggest.enabled": true,
    "editor.fontSize": 11,
    "editor.fontWeight": "600",
    "terminal.integrated.fontSize": 11,
    "terminal.integrated.fontWeightBold": "500",
    "workbench.preferredDarkColorTheme": "GitHub Dark",
    "workbench.tips.enabled": false,
    "workbench.colorCustomizations": {},
    "explorer.excludeGitIgnore": true,
    "explorer.enableUndo": false,
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": true,
        "markdown": true
    },
    "solidity.formatter": "forge",
    "terminal.integrated.fontFamily": "SauceCodePro NFM,Source Code Pro,monospace",
    "terminal.integrated.fontWeight": "normal",
    "terminal.integrated.shellIntegration.decorationsEnabled": "never",
    "explorer.compactFolders": false,
    "workbench.activityBar.visible": false,
    "workbench.iconTheme": "file-icons",
    "dotfiles.repository": "kalaspuff/dotfiles"
}